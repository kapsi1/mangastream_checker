{
  "name": "Mangastream checker",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Mangastream checker",
  "homepage_url": "https://github.com/kapsi1/mangastream_checker",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "http://mangastream.com/*",
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}